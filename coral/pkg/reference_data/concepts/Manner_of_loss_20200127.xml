    <skos:inScheme>
      <skos:ConceptScheme rdf:about="http://localhost:8000/5ea2235e-08b9-3e60-81fe-7918d1556ed7">
        <dcterms:identifier xml:lang="en">{"id": "a1ec7a6a-b5be-43ef-874b-189f78fe7ddd", "value": "http://purl.org/heritagedata/schemes/78"}</dcterms:identifier>
        <skos:hasTopConcept rdf:resource="http://localhost:8000/24edabea-f23d-3a67-b22a-4cc421ef4dd2"/>
        <dcterms:description xml:lang="en">{"id": "39115a23-1503-4e3b-a1bb-449fd75e0e0a", "value": "A small authority file/ wordlist for indexing types of loss of vessels or aircraft (e.g. beached, lost, enemy action)."}</dcterms:description>
        <dcterms:title xml:lang="en">{"id": "f47e91af-f48d-4392-a803-a401b06814cd", "value": "Manner of Loss"}</dcterms:title>
      </skos:ConceptScheme>
    </skos:inScheme>
    <skos:prefLabel xml:lang="en">{"id": "3d054fc6-16e0-42ec-9628-a1f61cb900d1", "value": "Mined"}</skos:prefLabel>
    <skos:scopeNote xml:lang="en">{"id": "62e41855-bbc0-4cc2-b391-b5d9c0d2d5ff", "value": "Loss due to action of free floating or tethered explosive device set to explode on contact with or in proximity to a vessel."}</skos:scopeNote>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/7469073f-7074-3b77-8738-92e4d926c7b9">
    <skos:prefLabel xml:lang="en">{"id": "fe95d1fd-3c25-4001-a651-7b7ed43608d5", "value": "Leaked"}</skos:prefLabel>
    <skos:scopeNote xml:lang="en">{"id": "83dadefd-3462-47c0-a87a-3c9c2da7167e", "value": "Ingress of water through damage or defects in the hull. Double-index with any cause, were known, e.g. COLLISION, and consequence, were known, e.g. BEACHED, FOUNDERED."}</skos:scopeNote>
    <skos:inScheme rdf:resource="http://localhost:8000/5ea2235e-08b9-3e60-81fe-7918d1556ed7"/>
    <dcterms:identifier xml:lang="en">{"id": "e5eeb108-0782-481b-b24d-582b70ed2593", "value": "http://purl.org/heritagedata/schemes/78/concepts/566"}</dcterms:identifier>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/beb2314d-0e07-3d21-bf47-5b12467440a5">
    <dcterms:identifier xml:lang="en">{"id": "eec3082c-30be-4cb2-ac5f-034a7f753622", "value": "http://purl.org/heritagedata/schemes/78/concepts/553"}</dcterms:identifier>
    <skos:inScheme rdf:resource="http://localhost:8000/5ea2235e-08b9-3e60-81fe-7918d1556ed7"/>
    <skos:scopeNote xml:lang="en">{"id": "a624c4d9-7ab9-4c74-a136-3363c9c017e2", "value": "A vessel that is deliberately run or dragged up the beach to save cargo and or crew. Do not use if the vessel is refloated."}</skos:scopeNote>
    <skos:prefLabel xml:lang="en">{"id": "157c50b7-41d7-4c90-b59a-c0f7a31585c9", "value": "Beached"}</skos:prefLabel>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/0f170590-1333-3540-bc0a-3df539817961">
    <skos:scopeNote xml:lang="en">{"id": "a5f7fc67-6534-4a13-b737-1c6e2cf9a861", "value": "Stoving in or fracturing of lowest part of vessel's hull."}</skos:scopeNote>
    <dcterms:identifier xml:lang="en">{"id": "87ca5c84-6778-45c0-b29b-478a76ea1fa9", "value": "http://purl.org/heritagedata/schemes/78/concepts/555"}</dcterms:identifier>
    <skos:inScheme rdf:resource="http://localhost:8000/5ea2235e-08b9-3e60-81fe-7918d1556ed7"/>
    <skos:prefLabel xml:lang="en">{"id": "df443349-a0f3-4a0b-8002-f570fe1a40c1", "value": "Bilged"}</skos:prefLabel>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/ec5863b1-3c36-3545-926c-72773ea5d3df">
    <skos:inScheme rdf:resource="http://localhost:8000/5ea2235e-08b9-3e60-81fe-7918d1556ed7"/>
    <skos:scopeNote xml:lang="en">{"id": "295afaa2-bee8-4e3f-857c-5f9ba65c4fc9", "value": "Refers to the abandonment of a vessel at sea due to its becoming unseaworthy.  Do not use for vessels abandoned having run aground. For aircraft use DITCHED."}</skos:scopeNote>
    <skos:prefLabel xml:lang="en">{"id": "5ff55921-4164-4e13-ab8a-f27648f74c6e", "value": "Abandoned"}</skos:prefLabel>
    <dcterms:identifier xml:lang="en">{"id": "4b5d39fc-c2ea-4706-86ac-3f57fc150203", "value": "http://purl.org/heritagedata/schemes/78/concepts/552"}</dcterms:identifier>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/036b2f3f-746a-3c26-8ddc-753666b96232">
    <skos:inScheme rdf:resource="http://localhost:8000/5ea2235e-08b9-3e60-81fe-7918d1556ed7"/>
    <skos:scopeNote xml:lang="en">{"id": "0dd7fd1d-186b-4f5c-a75e-7e92a833ab20", "value": "Loss of a vessel or aircraft primarily due to fire."}</skos:scopeNote>
    <skos:prefLabel xml:lang="en">{"id": "5030118d-d2ec-416d-9e38-2e865941d6e1", "value": "Burnt"}</skos:prefLabel>
    <dcterms:identifier xml:lang="en">{"id": "24707fa7-5b87-4d9f-8ac9-70a1c135e4b1", "value": "http://purl.org/heritagedata/schemes/78/concepts/557"}</dcterms:identifier>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/24edabea-f23d-3a67-b22a-4cc421ef4dd2">
    <dcterms:identifier xml:lang="en">{"id": "48c5490f-c531-4766-b6db-0aff986c0910", "value": "http://purl.org/heritagedata/schemes/78/concepts/100"}</dcterms:identifier>
    <skos:narrower>
      <skos:Concept rdf:about="http://localhost:8000/a4908cf6-258d-34b7-8968-4b4e4bee4b8e">
        <skos:inScheme rdf:resource="http://localhost:8000/5ea2235e-08b9-3e60-81fe-7918d1556ed7"/>
        <skos:prefLabel xml:lang="en">{"id": "db3c6390-d231-4af6-9a31-1464f1b77373", "value": "Mechanical Failure"}</skos:prefLabel>
        <skos:scopeNote xml:lang="en">{"id": "2683e229-369e-4dd5-9b44-6a732169be86", "value": "The loss of a craft due to structural or engine failure. In the case of loss resulting from the malfunction of navigational or operational apparatus, use INSTRUMENT FAILURE."}</skos:scopeNote>
        <dcterms:identifier xml:lang="en">{"id": "7f5c7106-c103-46a0-a0ec-1d16ec5d0c73", "value": "http://purl.org/heritagedata/schemes/78/concepts/1424"}</dcterms:identifier>
      </skos:Concept>
    </skos:narrower>
    <skos:narrower>
      <skos:Concept rdf:about="http://localhost:8000/fd2fe54f-c431-3848-869a-8ffe97b26df9">
        <skos:scopeNote xml:lang="en">{"id": "8b38bb0b-45cc-48b3-9a35-99c9325c4b35", "value": "A vessel which comes ashore and breaks up immediately due to severe weather."}</skos:scopeNote>
        <dcterms:identifier xml:lang="en">{"id": "c63777b7-c209-4a67-bf00-2f80f6f76095", "value": "http://purl.org/heritagedata/schemes/78/concepts/574"}</dcterms:identifier>
        <skos:inScheme rdf:resource="http://localhost:8000/5ea2235e-08b9-3e60-81fe-7918d1556ed7"/>
        <skos:prefLabel xml:lang="en">{"id": "c48975f1-c3d9-44de-9877-c8a7c19183cc", "value": "Wrecked"}</skos:prefLabel>
      </skos:Concept>
    </skos:narrower>
    <skos:narrower>
      <skos:Concept rdf:about="http://localhost:8000/bada0bc4-3e9f-3a42-abd8-bdf2f5d884af">
        <skos:inScheme rdf:resource="http://localhost:8000/5ea2235e-08b9-3e60-81fe-7918d1556ed7"/>
        <dcterms:identifier xml:lang="en">{"id": "7f1fddd7-e22a-43ad-b00a-400e17018ff0", "value": "http://purl.org/heritagedata/schemes/78/concepts/5215"}</dcterms:identifier>
        <skos:scopeNote xml:lang="en">{"id": "c9848263-dc4e-4e4d-a6cd-def6caa99d39", "value": "Acts of vandalism, or deliberate damage by owner or other parties, which result in the `Constructive Total Loss\u00ef\u00bf\u00bd (CTL) of vessel, or contributes to CTL of an abandoned vessel."}</skos:scopeNote>
        <skos:prefLabel xml:lang="en">{"id": "b25109ca-3698-47ff-89a1-4f90facb8e6b", "value": "Vandalism"}</skos:prefLabel>
      </skos:Concept>
    </skos:narrower>
    <skos:narrower>
      <skos:Concept rdf:about="http://localhost:8000/e65f89db-e9cc-39b4-a52f-2613f7ad190b">
        <dcterms:identifier xml:lang="en">{"id": "1db10bfe-e487-41c6-b52e-5e7c8e40d961", "value": "http://purl.org/heritagedata/schemes/78/concepts/556"}</dcterms:identifier>
        <skos:inScheme rdf:resource="http://localhost:8000/5ea2235e-08b9-3e60-81fe-7918d1556ed7"/>
        <skos:prefLabel xml:lang="en">{"id": "f26aeea5-7a56-424f-be24-a062af6f39f5", "value": "Broken Up"}</skos:prefLabel>
        <skos:scopeNote xml:lang="en">{"id": "6f5d46b6-3179-4868-bc45-58370edef536", "value": "A vessel which has been dismantled for the purpose of removal from the shore after stranding."}</skos:scopeNote>
      </skos:Concept>
    </skos:narrower>
    <skos:narrower>
      <skos:Concept rdf:about="http://localhost:8000/cfb016a3-7634-3b78-b4a2-ac4f4771c747">
        <dcterms:identifier xml:lang="en">{"id": "6a0f7b53-5fd8-44d3-b63e-7d82a080c310", "value": "http://purl.org/heritagedata/schemes/78/concepts/558"}</dcterms:identifier>
        <skos:inScheme rdf:resource="http://localhost:8000/5ea2235e-08b9-3e60-81fe-7918d1556ed7"/>
        <skos:prefLabel xml:lang="en">{"id": "15519ace-10d2-422f-aa81-01ba70e87988", "value": "Capsized"}</skos:prefLabel>
        <skos:scopeNote xml:lang="en">{"id": "a5deacd2-467f-4fe1-85af-c06fbdee7938", "value": "A vessel lost having overturned due to severe weather or shifting cargo."}</skos:scopeNote>
      </skos:Concept>
    </skos:narrower>
    <skos:inScheme rdf:resource="http://localhost:8000/5ea2235e-08b9-3e60-81fe-7918d1556ed7"/>
    <skos:narrower>
      <skos:Concept rdf:about="http://localhost:8000/4972ee65-a3b4-3d06-bc9f-de15197b5576">
        <skos:scopeNote xml:lang="en">{"id": "515e58d9-0e81-40fd-a93a-4955f3d37b19", "value": "Deliberate collision with another vessel in order to inflict damage."}</skos:scopeNote>
        <skos:inScheme rdf:resource="http://localhost:8000/5ea2235e-08b9-3e60-81fe-7918d1556ed7"/>
        <skos:prefLabel xml:lang="en">{"id": "26e63523-bad4-4fd2-89a8-149c98e670ec", "value": "Ramming"}</skos:prefLabel>
        <dcterms:identifier xml:lang="en">{"id": "72970e71-fb33-4346-b8d1-ee8b6651d853", "value": "http://purl.org/heritagedata/schemes/78/concepts/569"}</dcterms:identifier>
      </skos:Concept>
    </skos:narrower>
    <skos:narrower>
      <skos:Concept rdf:about="http://localhost:8000/3924e300-a6fe-3258-ab0a-940bdb5252ee">
        <skos:prefLabel xml:lang="en">{"id": "cb06a4bb-0345-4f9f-8d5f-9650b4e15b06", "value": "Back Broken"}</skos:prefLabel>
        <dcterms:identifier xml:lang="en">{"id": "4ea732f7-459c-4cfb-9556-0fcc90372e13", "value": "http://purl.org/heritagedata/schemes/78/concepts/5214"}</dcterms:identifier>
        <skos:inScheme rdf:resource="http://localhost:8000/5ea2235e-08b9-3e60-81fe-7918d1556ed7"/>
        <skos:scopeNote xml:lang="en">{"id": "8b7b477d-cf98-4a01-8bef-54e693d94b07", "value": "Use when the vessel splits in two due to stresses on her frame where she lies unsupported either amidships or at the ends, typically after stranding, although this may also happen in similar conditions on the seabed after foundering."}</skos:scopeNote>
      </skos:Concept>
    </skos:narrower>
    <skos:narrower>
      <skos:Concept rdf:about="http://localhost:8000/499edd53-748f-3768-80a0-4aa66ccdb551">
        <skos:scopeNote xml:lang="en">{"id": "c629f943-e1d3-45ee-a12f-3f2ccf8b362d", "value": "Use where a section of the vessel has been recovered from where she lies, either at the time or subsequently, leaving behind substantial remains (unlike RECOVERED, where the entire vessel has been removed from the site)."}</skos:scopeNote>
        <skos:prefLabel xml:lang="en">{"id": "1f118354-9266-4a22-95e1-d84f0cc69dd3", "value": "Partially Recovered"}</skos:prefLabel>
        <skos:inScheme rdf:resource="http://localhost:8000/5ea2235e-08b9-3e60-81fe-7918d1556ed7"/>
        <dcterms:identifier xml:lang="en">{"id": "486f0da4-a7c6-4320-895e-02c66570cb60", "value": "http://purl.org/heritagedata/schemes/78/concepts/5213"}</dcterms:identifier>
      </skos:Concept>
    </skos:narrower>
    <skos:narrower>
      <skos:Concept rdf:about="http://localhost:8000/e4ea902e-e377-37df-a1d4-a4e39353dc94">
        <skos:prefLabel xml:lang="en">{"id": "c85aba81-c984-4538-983b-903f9a4a8fb9", "value": "Torpedoed"}</skos:prefLabel>
        <skos:inScheme rdf:resource="http://localhost:8000/5ea2235e-08b9-3e60-81fe-7918d1556ed7"/>
        <skos:scopeNote xml:lang="en">{"id": "40535986-1715-4bc4-87ff-7244fb9a59b2", "value": "Loss due to action of self propelled submerged missile."}</skos:scopeNote>
        <dcterms:identifier xml:lang="en">{"id": "23d7c77a-721e-4015-b3d3-b5f4f614b4fb", "value": "http://purl.org/heritagedata/schemes/78/concepts/573"}</dcterms:identifier>
      </skos:Concept>
    </skos:narrower>
    <skos:narrower>
      <skos:Concept rdf:about="http://localhost:8000/d0d3b35f-144b-315a-9570-de0febc171f3">
        <skos:inScheme rdf:resource="http://localhost:8000/5ea2235e-08b9-3e60-81fe-7918d1556ed7"/>
        <skos:scopeNote xml:lang="en">{"id": "2e11a2b5-1d78-4cfc-9f13-d01f2f0ef2bf", "value": "Use where an entire, wrecked or incapacitated, craft has been recovered or salvaged, possibly leaving debris or other evidence of its presence at the wreck or crash site. If part of the wreck remains on the seabed use PARTIALLY RECOVERED."}</skos:scopeNote>
        <dcterms:identifier xml:lang="en">{"id": "9a93034f-8884-41e8-bfa4-6b510fdd7cd8", "value": "http://purl.org/heritagedata/schemes/78/concepts/5065"}</dcterms:identifier>
        <skos:prefLabel xml:lang="en">{"id": "19d71e5e-dd40-4621-86ab-adc436b7f708", "value": "Recovered"}</skos:prefLabel>
      </skos:Concept>
    </skos:narrower>
    <skos:prefLabel xml:lang="en">{"id": "850c1edd-7b2e-4232-be2c-4a490eba2693", "value": "Manner of Loss"}</skos:prefLabel>
    <skos:narrower rdf:resource="http://localhost:8000/beb2314d-0e07-3d21-bf47-5b12467440a5"/>
    <skos:narrower>
      <skos:Concept rdf:about="http://localhost:8000/1111eb4c-bd09-3a16-8f95-109fe86b87b3">
        <skos:scopeNote xml:lang="en">{"id": "0e4461c9-6950-48dc-84ac-61e2620f7723", "value": "Use only for fragmentary wreckage washed up at the time of loss or subsequently: but not for complete hulls for which STRANDED or another appropriate manner of loss should be used."}</skos:scopeNote>
        <dcterms:identifier xml:lang="en">{"id": "d1c9d978-9304-4fbd-bd46-3b0a954c71bc", "value": "http://purl.org/heritagedata/schemes/78/concepts/5238"}</dcterms:identifier>
        <skos:prefLabel xml:lang="en">{"id": "f38ee20c-2eca-48e9-8446-1da8bbc00215", "value": "Washed Up"}</skos:prefLabel>
        <skos:inScheme rdf:resource="http://localhost:8000/5ea2235e-08b9-3e60-81fe-7918d1556ed7"/>
      </skos:Concept>
    </skos:narrower>
    <skos:narrower>
      <skos:Concept rdf:about="http://localhost:8000/9af0feae-a360-3a67-b6a7-5b4b6235c357">
        <skos:inScheme rdf:resource="http://localhost:8000/5ea2235e-08b9-3e60-81fe-7918d1556ed7"/>
        <dcterms:identifier xml:lang="en">{"id": "139e97d9-d044-433f-884e-334d5548e62f", "value": "http://purl.org/heritagedata/schemes/78/concepts/1425"}</dcterms:identifier>
        <skos:scopeNote xml:lang="en">{"id": "3b5df07c-3ac5-4a13-af60-10fff1ea04c6", "value": "An aircraft lost due to offensive action. Includes air-to-air and ground-to-air combat. For Vessels use GUN ACTION."}</skos:scopeNote>
        <skos:prefLabel xml:lang="en">{"id": "8f105ee3-82dc-4d8e-b39a-1b4aaf0a80f6", "value": "Shot Down"}</skos:prefLabel>
      </skos:Concept>
    </skos:narrower>
    <skos:narrower>
      <skos:Concept rdf:about="http://localhost:8000/22283369-35d1-37ba-b973-4b51ec99ace0">
        <skos:scopeNote xml:lang="en">{"id": "ecdb46a2-27ce-44e0-bf28-56da36e410f6", "value": "Reuse of timbers or hull of a vessel in construction. For example sinking of hulks as harbour foundations or recycling of timbers in domestic contexts."}</skos:scopeNote>
        <dcterms:identifier xml:lang="en">{"id": "f5ebe0fc-5431-4957-866a-c575c0f776f7", "value": "http://purl.org/heritagedata/schemes/78/concepts/5240"}</dcterms:identifier>
        <skos:prefLabel xml:lang="en">{"id": "23b99b21-fe95-4cb2-abb4-f112f1f8f673", "value": "Used In Construction"}</skos:prefLabel>
        <skos:inScheme rdf:resource="http://localhost:8000/5ea2235e-08b9-3e60-81fe-7918d1556ed7"/>
      </skos:Concept>
    </skos:narrower>
    <skos:narrower>
      <skos:Concept rdf:about="http://localhost:8000/edc2c56b-cd3f-3714-8df4-da6bce98c763">
        <skos:inScheme rdf:resource="http://localhost:8000/5ea2235e-08b9-3e60-81fe-7918d1556ed7"/>
        <skos:prefLabel xml:lang="en">{"id": "19b9d3b6-7f24-4e39-a6e0-aad88d493129", "value": "Lightning Strike"}</skos:prefLabel>
        <dcterms:identifier xml:lang="en">{"id": "3a471681-d516-4fec-9a01-cd5f15dc9aca", "value": "http://purl.org/heritagedata/schemes/78/concepts/5217"}</dcterms:identifier>
        <skos:scopeNote xml:lang="en">{"id": "09ba2e7b-32e1-40fe-9217-e3556af3b507", "value": "A vessel which has damaged by a lightning strike and subsequently lost."}</skos:scopeNote>
      </skos:Concept>
    </skos:narrower>
    <skos:narrower rdf:resource="http://localhost:8000/0f170590-1333-3540-bc0a-3df539817961"/>
    <skos:narrower>
      <skos:Concept rdf:about="http://localhost:8000/0f013352-1d62-3539-95a7-c67da61a726e">
        <dcterms:identifier xml:lang="en">{"id": "8f1abeb9-821b-4871-98f6-4589b93c4b74", "value": "http://purl.org/heritagedata/schemes/78/concepts/567"}</dcterms:identifier>
        <skos:inScheme rdf:resource="http://localhost:8000/5ea2235e-08b9-3e60-81fe-7918d1556ed7"/>
        <skos:scopeNote xml:lang="en">{"id": "64a7eb52-d5f8-4b9a-aaf2-1b851d33a947", "value": "A vessel or aircraft lost at sea, cause unknown."}</skos:scopeNote>
        <skos:prefLabel xml:lang="en">{"id": "65156236-ae25-4f35-a8fa-bd4790b9416a", "value": "Lost"}</skos:prefLabel>
      </skos:Concept>
    </skos:narrower>
    <skos:narrower>
      <skos:Concept rdf:about="http://localhost:8000/314bae16-9857-3970-af12-38276c22db52">
        <dcterms:identifier xml:lang="en">{"id": "59caa427-40b0-4eb8-a60a-9c474e5dac55", "value": "http://purl.org/heritagedata/schemes/78/concepts/5198"}</dcterms:identifier>
        <skos:prefLabel xml:lang="en">{"id": "96ce2c2f-9034-4522-a586-23ffe4e66929", "value": "Condemned"}</skos:prefLabel>
        <skos:inScheme rdf:resource="http://localhost:8000/5ea2235e-08b9-3e60-81fe-7918d1556ed7"/>
